<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/loginFragment">
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.dirion.walltechtodo.view.features.login.LoginFragment"
        android:label="LoginFragment"
        tools:layout="@layout/fragment_login">
        <action
            android:id="@+id/action_loginFragment_to_tasksFragment"
            app:destination="@id/tasksFragment"
            app:launchSingleTop="true"
            app:popUpTo="@+id/nav_graph"
            app:popUpToInclusive="true"
            />
    </fragment>
    <fragment
        android:id="@+id/addTaskFragment"
        android:name="com.dirion.walltechtodo.view.features.add_order.AddOrderFragment"
        android:label="AddTaskFragment"
        tools:layout="@layout/fragment_add_task"
        >
        <action
            android:id="@+id/action_addTaskFragment_to_tasksFragment"
            app:destination="@id/tasksFragment" />
    </fragment>
    <fragment
        android:id="@+id/dataTimeFragment"
        android:name="com.dirion.walltechtodo.view.features.data_time.DateTimeFragment"
        android:label="DataTimeFragment"
        tools:layout="@layout/fragment_date_time"
        >
        <action
            android:id="@+id/action_dataTimeFragment_to_settingsFragment"
            app:destination="@id/settingsFragment" />
    </fragment>
    <fragment
        android:id="@+id/editTaskFragment"
        android:name="com.dirion.walltechtodo.view.features.edit_order.EditOrderFragment"
        android:label="EditTaskFragment"
        tools:layout="@layout/fragment_edit_task"
        >
        <action
            android:id="@+id/action_editTaskFragment_to_tasksFragment"
            app:destination="@id/tasksFragment" />
    </fragment>
    <fragment
        android:id="@+id/notesFragment"
        android:name="com.dirion.walltechtodo.view.features.notes.NotesFragment"
        android:label="NotesFragment"
        tools:layout="@layout/fragment_notes"
        >
        <action
            android:id="@+id/action_notesFragment_to_settingsFragment"
            app:destination="@id/settingsFragment" />
    </fragment>
    <fragment
        android:id="@+id/notificationsFragment"
        android:name="com.dirion.walltechtodo.view.features.notification.NotificationsFragment"
        android:label="NotificationsFragment"
        tools:layout="@layout/fragment_notifications"
        >
        <action
            android:id="@+id/action_notificationsFragment_to_settingsFragment"
            app:destination="@id/settingsFragment" />
    </fragment>
    <fragment
        android:id="@+id/settingsFragment"
        android:name="com.dirion.walltechtodo.view.features.settings.SettingsFragment"
        android:label="SettingsFragment"
        tools:layout="@layout/fragment_settings"
        >
        <action
            android:id="@+id/action_settingsFragment_to_notificationsFragment"
            app:destination="@id/notificationsFragment" />
        <action
            android:id="@+id/action_settingsFragment_to_dataTimeFragment"
            app:destination="@id/dataTimeFragment" />
        <action
            android:id="@+id/action_settingsFragment_to_notesFragment"
            app:destination="@id/notesFragment" />
        <action
            android:id="@+id/action_settingsFragment_to_namesFragment"
            app:destination="@id/nameFragment" />
        <action
            android:id="@+id/action_settingsFragment_to_volumeFragment"
            app:destination="@id/volumeFragment" />
        <action
            android:id="@+id/action_settingsFragment_to_tasksFragment"
            app:destination="@id/tasksFragment" />
        <action
            android:id="@+id/action_settingsFragment_to_nameFragment"
            app:destination="@id/nameFragment" />
        <action
            android:id="@+id/action_settingsFragment_to_loginFragment"
            app:destination="@id/loginFragment"
            app:launchSingleTop="true"
            app:popUpTo="@+id/nav_graph"
            app:popUpToInclusive="true"
            />
    </fragment>
    <fragment
        android:id="@+id/tasksFragment"
        android:name="com.dirion.walltechtodo.view.features.orders.OrdersFragment"
        android:label="TasksFragment"
        tools:layout="@layout/fragment_tasks"
        >
        <action
            android:id="@+id/action_tasksFragment_to_addTaskFragment"
            app:destination="@id/addTaskFragment" />
        <action
            android:id="@+id/action_tasksFragment_to_editTaskFragment"
            app:destination="@id/editTaskFragment" />
        <action
            android:id="@+id/action_tasksFragment_to_settingsFragment"
            app:destination="@id/settingsFragment" />
        <action
            android:id="@+id/action_tasksFragment_to_typographyFragment"
            app:destination="@id/typographyFragment" />
        <action
            android:id="@+id/action_tasksFragment_to_lkFragment"
            app:destination="@id/lkFragment" />
        <action
            android:id="@+id/action_tasksFragment_to_customersFragment"
            app:destination="@id/customersFragment" />
        <action
            android:id="@+id/action_tasksFragment_to_employersFragment"
            app:destination="@id/employersFragment" />
        <action
            android:id="@+id/action_tasksFragment_to_myOrdersFragment"
            app:destination="@id/myOrdersFragment" />
    </fragment>
    <fragment
        android:id="@+id/volumeFragment"
        android:name="com.dirion.walltechtodo.view.features.volume.VolumeFragment"
        android:label="VolumeFragment"
        tools:layout="@layout/fragment_volume"
        >
        <action
            android:id="@+id/action_volumeFragment_to_settingsFragment"
            app:destination="@id/settingsFragment" />
    </fragment>
    <fragment
        android:id="@+id/nameFragment"
        android:name="com.dirion.walltechtodo.view.features.name.NameFragment"
        android:label="NameFragment"
        tools:layout="@layout/fragment_name"
        >
        <action
            android:id="@+id/action_nameFragment_to_settingsFragment"
            app:destination="@id/settingsFragment" />
    </fragment>
    <fragment
        android:id="@+id/typographyFragment"
        android:name="com.dirion.walltechtodo.view.features.typography.TypographyFragment"
        android:label="TypographyFragment" >
        <action
            android:id="@+id/action_typographyFragment_to_customersFragment"
            app:destination="@id/customersFragment" />
        <action
            android:id="@+id/action_typographyFragment_to_lkFragment"
            app:destination="@id/lkFragment" />
        <action
            android:id="@+id/action_typographyFragment_to_employersFragment"
            app:destination="@id/employersFragment" />
        <action
            android:id="@+id/action_typographyFragment_to_myOrdersFragment"
            app:destination="@id/myOrdersFragment" />
    </fragment>
    <fragment
        android:id="@+id/lkFragment"
        android:name="com.dirion.walltechtodo.view.features.lk.LkFragment"
        android:label="LkFragment" >
        <action
            android:id="@+id/action_lkFragment_to_tasksFragment"
            app:destination="@id/tasksFragment" />
        <action
            android:id="@+id/action_lkFragment_to_customersFragment"
            app:destination="@id/customersFragment" />
        <action
            android:id="@+id/action_lkFragment_to_typographyFragment"
            app:destination="@id/typographyFragment" />
        <action
            android:id="@+id/action_lkFragment_to_employersFragment"
            app:destination="@id/employersFragment" />
        <action
            android:id="@+id/action_lkFragment_to_myOrdersFragment"
            app:destination="@id/myOrdersFragment" />
    </fragment>
    <fragment
        android:id="@+id/customersFragment"
        android:name="com.dirion.walltechtodo.view.features.customers.CustomersFragment"
        android:label="CustomersFragment" >
        <action
            android:id="@+id/action_customersFragment_to_tasksFragment"
            app:destination="@id/tasksFragment" />
        <action
            android:id="@+id/action_customersFragment_to_lkFragment"
            app:destination="@id/lkFragment" />
        <action
            android:id="@+id/action_customersFragment_to_typographyFragment"
            app:destination="@id/typographyFragment" />
        <action
            android:id="@+id/action_customersFragment_to_employersFragment"
            app:destination="@id/employersFragment" />
        <action
            android:id="@+id/action_customersFragment_to_myOrdersFragment"
            app:destination="@id/myOrdersFragment" />
    </fragment>
    <fragment
        android:id="@+id/employersFragment"
        android:name="com.dirion.walltechtodo.view.features.employers.EmployersFragment"
        android:label="EmployersFragment" >
        <action
            android:id="@+id/action_employersFragment_to_tasksFragment"
            app:destination="@id/tasksFragment" />
        <action
            android:id="@+id/action_employersFragment_to_lkFragment"
            app:destination="@id/lkFragment" />
        <action
            android:id="@+id/action_employersFragment_to_customersFragment"
            app:destination="@id/customersFragment" />
        <action
            android:id="@+id/action_employersFragment_to_typographyFragment"
            app:destination="@id/typographyFragment" />
        <action
            android:id="@+id/action_employersFragment_to_myOrdersFragment"
            app:destination="@id/myOrdersFragment" />
    </fragment>
    <fragment
        android:id="@+id/myOrdersFragment"
        android:name="com.dirion.walltechtodo.view.features.my_orders.MyOrdersFragment"
        android:label="MyOrdersFragment" >
        <action
            android:id="@+id/action_myOrdersFragment_to_tasksFragment"
            app:destination="@id/tasksFragment" />
        <action
            android:id="@+id/action_myOrdersFragment_to_lkFragment"
            app:destination="@id/lkFragment" />
        <action
            android:id="@+id/action_myOrdersFragment_to_customersFragment"
            app:destination="@id/customersFragment" />
        <action
            android:id="@+id/action_myOrdersFragment_to_typographyFragment"
            app:destination="@id/typographyFragment" />
        <action
            android:id="@+id/action_myOrdersFragment_to_employersFragment"
            app:destination="@id/employersFragment" />
    </fragment>

</navigation>